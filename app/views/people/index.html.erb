<table>
  <tr>
    <th>
      Name
    </th>
    <th>
      Address
    </th>
  </tr>
  <% @people.each do |person| %>
      <tr>
        <td><%= person.name %></td>
        <td><%= person.address %></td>
        <td><%= person.dob %></td>
        <td><%= person.aadharNum %></td>
      </tr>
  <% end %>

</table>
